{
  "name": "lnkbrd",
  "version": "1.0.0",
  "description": "Link Bird is an open-source API-first URL shortener",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "npm run build && node dist/main.js",
    "lint": "prettier --write --config prettier.config.js --list-different src/**/*.ts",
    "start": "node ./dist/main.js",
    "test": "tsc && jest --coverage --testPathPattern ./dist/.*\\.test\\.js$"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.6",
    "@types/qs": "^6.9.18",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@faker-js/faker": "^9.3.0",
    "@fastify/cors": "^10.0.2",
    "@fastify/swagger": "^9.4.1",
    "@fastify/swagger-ui": "^5.2.1",
    "@fastify/view": "^10.0.2",
    "axios": "^1.7.9",
    "cheerio": "^1.0.0",
    "dotenv": "^16.4.7",
    "fastify": "^5.2.1",
    "handlebars": "^4.7.8",
    "ip3country": "^5.0.0",
    "isbot": "^5.1.21",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.12.0",
    "posthog-node": "^4.8.1",
    "qs": "^6.14.0",
    "ua-parser-js": "^2.0.0"
  }
