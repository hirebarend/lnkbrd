    "@types/qs": "^6.14.0",
    "jest": "^30.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@faker-js/faker": "^9.8.0",
    "@fastify/cors": "^11.0.1",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@fastify/view": "^11.1.0",
    "axios": "^1.9.0",
    "cheerio": "^1.1.0",
    "dotenv": "^16.5.0",
    "fastify": "^5.4.0",
    "handlebars": "^4.7.8",
    "ip3country": "^5.0.0",
    "isbot": "^5.1.28",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.17.0",
    "posthog-node": "^5.1.0",
    "qrcode": "^1.5.4",
    "qs": "^6.14.0",
    "ua-parser-js": "^2.0.3"
  }
